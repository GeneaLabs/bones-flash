{
    "name": "genealabs/laravel-messenger",
    "description": "Flash notification module for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5 - 6.0",
        "illuminate/session": "5.5 - 6.0",
        "jenssegers/model": "*"
    },
    "require-dev": {
        "laravel/laravel": "dev-develop@dev",
        "mockery/mockery": "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "*",
        "php-coveralls/php-coveralls" : "*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/laravel/laravel/app/",
            "GeneaLabs\\LaravelMessenger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelMessenger\\Providers\\Service"
            ],
            "aliases": {
                "Messenger": "GeneaLabs\\LaravelMessenger\\Facades\\Messenger"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
