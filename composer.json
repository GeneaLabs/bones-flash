{
    "name": "genealabs/laravel-messenger",
    "description": "Flash notification module for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.5.*",
        "illuminate/session": "5.5.*"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
        "fzaninotto/faker": "~1.4",
        "illuminate/routing": "5.5.*",
        "laravel/laravel": "5.5.*",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "5.7.*",
        "satooshi/php-coveralls" : "dev-master@dev",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/laravel/laravel/app/",
            "GeneaLabs\\LaravelMessenger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelMessenger\\Providers\\Service"
            ],
            "aliases": {
                "Messenger": "GeneaLabs\\LaravelMessenger\\Facades\\Messenger"
            }
        }
    },
    "minimum-stability": "stable"
}
