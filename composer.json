{
    "name": "genealabs/laravel-messenger",
    "description": "Flash notification module for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.5.*",
        "illuminate/session": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "sebastian/phpcpd": "^3.0",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelMessenger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelMessenger\\Providers\\Service"
            ],
            "aliases": {
                "Messenger": "GeneaLabs\\LaravelMessenger\\Facades\\Messenger"
            }
        }
    },
    "minimum-stability": "stable"
}
